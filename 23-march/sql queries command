CREATE TABLE `Singer` (
	id int(10) auto_increment PRIMARY KEY,
	singer_name varchar(50) unique key,
	Country varchar(30) NOT NULL,
	singing_lang varchar(30) NOT NULL,
	famous_song varchar(60) NOT NULL
);


