create table `Games` (
    Gamename varchar(30),
    teams varchar(30) NOT NULL,
    Gameday date NOT NULL,
    venue varchar(30) NOT NULL,
    result varchar(20),
    result_date date NOT NULL 
);

Alter table Games modify result varchar(30) default 'draw';

