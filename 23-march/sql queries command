CREATE TABLE `professors` (
	emp_id int(10) auto_increment PRIMARY KEY,
	pro_name varchar(30) NOT NULL,
	designation varchar(30) NOT NULL,
	subject varchar(30) NOT NULL,
	contact_num bigint(30) NOT NULL
);

