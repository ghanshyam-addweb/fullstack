create table `Bank` (
Acc_no bigint(100) NOT NULL primary key,
Acc_Holder_Name varchar(60) NOT NULL,
Acc_Balance bigint(30) NOT NULL,
Acc_Type varchar(30) NOT NULL default 'Saving',
Branch varchar(30) NOT NULL,
Mobile bigint(30) unique key
);


