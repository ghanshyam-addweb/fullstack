CREATE TABLE `stores` (
	store_id int(10) auto_increment PRIMARY KEY,
	store_name varchar(30) NOT NULL,
	phone bigint(30) NOT NULL,
	email varchar(30),
	Address varchar(30) NOT NULL
);

